# Backend Environment Variables Template
# Copy this file to .env and fill in your actual values

# Server Port
PORT=5000

# MongoDB Connection String
# For local development: mongodb://localhost:27017/vivante_air
# For production: Your MongoDB Atlas connection string
MONGODB_URI=mongodb://localhost:27017/vivante_air

# JWT Secret Key (change this to a strong random string in production!)
JWT_SECRET=your_jwt_secret_key_here
